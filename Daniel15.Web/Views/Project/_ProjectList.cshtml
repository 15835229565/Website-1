@using Daniel15.Web.ViewModels.Project
@using Microsoft.AspNet.Mvc.Rendering
@model ProjectListViewModel

<ul class="projects">
	@foreach (var project in Model.Projects)
	{
		@Html.Partial("_Project", new ProjectViewModel
		{
			Project = project,
			Technologies = project.Technologies.Select(slug => Model.Technologies[slug]),
		});
	}
</ul>